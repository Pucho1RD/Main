-- Obtener el ID del juego actual
local currentPlaceId = game.PlaceId

-- Tabla con los IDs de los juegos específicos
local gameIds = {
    [18138547215] = "https://raw.githubusercontent.com/Pucho1RD/Anime-Simulator/main/Katanga",
    [17316900493] = "loadstring(game:HttpGet("https://raw.githubusercontent.com/Pucho1RD/Katanga/main/Hub"))()
" -- Añade más IDs y URLs según sea necesario
}

-- Verifica si el PlaceId actual está en la tabla y carga el script correspondiente
local scriptUrl = gameIds[currentPlaceId]
if scriptUrl then
    loadstring(game:HttpGet(scriptUrl))()
else
    print("El PlaceId actual no coincide con ningún ID en la tabla.")
end
